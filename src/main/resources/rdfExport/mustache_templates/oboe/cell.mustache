#Row {{rowModel.rowId}}, Column {{columnModel.columnHeader}}
{{!Construct an oboe-core:Measurement for the cell}}
:meas_{{cellEntityIdentifier}} rdf:type oboe-core:Measurement .
{{!Relate the Measurement to the Observation}}
:obs_{{columnModel.columnEntityIdentifier}} oboe-core:hasMeasurement :meas_{{cellEntityIdentifier}} .
{{!Construct an oboe-core:MeasuredValue for the cell}}
:measuredValue_{{cellEntityIdentifier}} rdf:type oboe-core:MeasuredValue .
{{!Relate the Measurement to the MeasuredValue}}
:meas_{{cellEntityIdentifier}} oboe-core:hasValue :measuredValue_{{cellEntityIdentifier}} .

{{!Use semantic information}}
{{!Creator - optional}}
{{#semantics.Creator}}
{{!Given a literal value}}
    {{#semantics.Creator.isLiteral}}
        :meas_{{cellEntityIdentifier}} dcterms:creator :creator_{{cellEntityIdentifier}} .
        :creator_{{cellEntityIdentifier}} rdf:type foaf:Person .
        :creator_{{cellEntityIdentifier}} foaf:name "{{{semantics.Creator.value}}}" .
    {{/semantics.Creator.isLiteral}}
    {{!Given an RDF resource}}
    {{^semantics.Creator.isLiteral}}
        :meas_{{cellEntityIdentifier}} dcterms:creator <{{{semantics.Creator.value}}}> .
    {{/semantics.Creator.isLiteral}}
{{/semantics.Creator}}

{{!Characteristic - required}}
{{!Given a literal value}}
{{#semantics.Characteristic.isLiteral}}
    :meas_{{cellEntityIdentifier}} oboe-core:ofCharacteristic "{{{semantics.Characteristic.value}}}" .
{{/semantics.Characteristic.isLiteral}}
{{!Given an RDF resource}}
{{^semantics.Characteristic.isLiteral}}
    :meas_{{cellEntityIdentifier}} oboe-core:ofCharacteristic <{{{semantics.Characteristic.value}}}> .
{{/semantics.Characteristic.isLiteral}}

{{!Unit - required}}
{{!Given a literal value}}
{{#semantics.Unit.isLiteral}}
    :meas_{{cellEntityIdentifier}} om:hasUnit "{{{semantics.Unit.value}}}" .
{{/semantics.Unit.isLiteral}}
{{!Given an RDF resource}}
{{^semantics.Unit.isLiteral}}
    :meas_{{cellEntityIdentifier}} om:hasUnit <{{{semantics.Unit.value}}}> .
{{/semantics.Unit.isLiteral}}

